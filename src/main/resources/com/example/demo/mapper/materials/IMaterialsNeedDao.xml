<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.department.dao.IDepartmentDao">

	<insert id="createMaterialNeeds" useGeneratedKeys="true" keyProperty="id" keyColumn="id" parameterType="java.util.List">
		 INSERT INTO materials_need_t (
			  `id`,
			  `title`,
			  `materials_sort`,
			  `materials_code`,
			  `need_count`,
			  `need_date`,
			  `need_unit`,
			  `technical_parameters`,
			  `remark`,
			  `creater_by`,
			  `create_date`
			) 
			VALUES
		<foreach collection="list" item="list" index="index" separator=",">
  			(#{list.id},
  			 #{list.title},
  			 #{list.materialsSort},
  			 #{list.needCount},
  			 #{list.needDate},
  			 #{list.needUnit},
  			 #{list.technicalParameters},
  			 #{list.remark},
  			 #{list.createrBy},
  			 now())
        </foreach>
	</insert>
	<delete id="deleteMaterialNeeds">
		delete materials_need_t 
	</delete>
</mapper>